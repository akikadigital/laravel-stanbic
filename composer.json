{
    "name": "akika/laravel-stanbic",
    "description": "An unofficial package for Stanbic",
    "keywords": [
        "Akika",
        "laravel",
        "laravel-stanbic"
    ],
    "homepage": "https://github.com/akika/laravel-stanbic",
    "license": "MIT",
    "authors": [
        {
            "name": "Akika Digital",
            "email": "akika.digital@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8.0||^10.0||^11.0||^12.0",
        "illuminate/support": "^8.0||^10.0||^11.0||^12.0",
        "saloonphp/xml-wrangler": "^1.4",
        "spatie/laravel-package-tools": "^1.16||^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0||^6.0.0",
        "pestphp/pest": "^3.0||^2.4||^1.23",
        "pestphp/pest-plugin-arch": "^3.0||^2.4||^1.23",
        "pestphp/pest-plugin-laravel": "^3.0||^2.4||^1.23",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0"
    },
    "autoload": {
        "psr-4": {
            "Akika\\LaravelStanbic\\": "src/",
            "Akika\\LaravelStanbic\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akika\\LaravelStanbic\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": [
            "@php vendor/bin/testbench package:discover --ansi",
            "cp .hooks/pre-commit .git/hooks/pre-commit"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akika\\LaravelStanbic\\LaravelStanbicServiceProvider"
            ],
            "aliases": {
                "LaravelStanbic": "Akika\\LaravelStanbic\\Facades\\LaravelStanbic"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
